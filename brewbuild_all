#!/bin/bash

set -e

perls="
    5.6.1 5.6.2
    5.8.0 5.8.1 5.8.2 5.8.3 5.8.4 5.8.5 5.8.6 5.8.7 5.8.8 5.8.9
    5.10.0 5.10.1
    5.12.0 5.12.1 5.12.2 5.12.3 5.12.4 5.12.5
    5.14.0 5.14.1 5.14.2 5.14.3 5.14.4
    5.16.0 5.16.1 5.16.2 5.16.3
    5.18.0 5.18.1 5.18.2
    5.20.0 5.20.1 5.20.2 5.20.3
    5.22.0 5.22.1
    5.23.0 5.23.1 5.23.2 5.23.3 5.23.4 5.23.5 5.23.6
    5.23.7 5.23.8 5.23.9
"

dir=$(pwd)
echo dir $dir

parallel $dir/brewbuild :::: $perls

perlbrew clean
